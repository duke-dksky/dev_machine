[alias]
    lg      = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    ci      = commit
    co      = checkout
    br      = branch
    st      = status
    list    = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    undo    = reset --hard
    llog    = log --date=local
    changes = diff --name-status -r
    diffst  = diff --stat -r
    ignore  = !git ls-files -v | grep "^[[:lower:]]"
    conf    = diff --name-only --diff-filter=U
[user]
	name    = yangdongkui
	email   = yangdongkui928@gmail.com
[http]
	sslVerify = false
[core]
	editer = vim
	editor = vim
[url "git@git.ucloudadmin.com:"]
	insteadOf = https://git.ucloudadmin.com/
